{
  "name": "Unblock Youku",
  "version": "2.0.2",
  "manifest_version": 2,
  "minimum_chrome_version": "18.0",
  "description": "Let you smoothly surf on many websites blocking visitors outside Mainland China.",
  "icons": { "16": "icons/unblock-youku-16.png",
             "48": "icons/unblock-youku-48.png",
            "128": "icons/unblock-youku-128.png"
  },
  "permissions": [
    "proxy",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "proxy.js",
      "header.js"
    ]
  }
}
