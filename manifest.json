{
  "name": "Unblock Youku",
  "version": "2.3.3",
  "manifest_version": 2,
  "minimum_chrome_version": "18.0",
  "description": "Let you smoothly surf on many websites blocking visitors outside mainland China.",
  "icons": { "16": "icons/16.png",
             "48": "icons/48.png",
            "128": "icons/128.png"
  },
  "permissions": [
    "proxy",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "proxy.js",
      "header.js"
    ]
  }
}
