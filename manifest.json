{
  "name": "Unblock Youku",
  "version": "1.2",
  "description": "A Chrome extension for viewing Youku/Tudou online videos from overseas (without proxy!)",
  "icons": { "16": "unblock-youku-16.png",
             "48": "unblock-youku-48.png",
            "128": "unblock-youku-128.png" },
  "permissions": [
    "webRequest", "webRequestBlocking",
    "http://*.youku.com/*",
    "http://*.tudou.com/*",
    "http://tudou.letv.com/*",
    "http://*.tudou.letv.com/*",
    "http://*.xiami.com/*"
  ],
  "background_page": "background.html"
}
